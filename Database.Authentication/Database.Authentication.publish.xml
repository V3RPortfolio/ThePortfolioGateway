<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>portfolio_auth</TargetDatabaseName>
    <DeployScriptFileName>Database.Authentication.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=127.0.0.1;User ID=sa;Password=Admin@123;Persist Security Info=False;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=True;Authentication=SqlPassword</TargetConnectionString>
    <!-- <TargetConnectionString>Data Source=localhost;Integrated Security=True;Persist Security Info=False;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=True</TargetConnectionString> -->
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <IgnoreRoleMembership>True</IgnoreRoleMembership>
    <ExcludeUsers>True</ExcludeUsers>
    <ExcludeLogins>True</ExcludeLogins>
    <DoNotAlterReplicatedObjects>False</DoNotAlterReplicatedObjects>
  </PropertyGroup>
</Project>